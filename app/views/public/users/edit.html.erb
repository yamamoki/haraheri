<h1>Public::Users#edit</h1>
<p>Find me in app/views/public/users/edit.html.erb</p>

<h2>プロフィール編集</h2>
<%= form_with model: @user, url: users_path do |f| %>
  <label for="inputName">名前</label>
  <%= f.text_field :name, autofocus: true, id:"inputName" %>
  <%= f.text_field :email, autofocus: true, id:"inputEmail" %>
  <%= f.submit "変更を保存" %>
<% end %>
