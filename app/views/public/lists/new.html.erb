<h1>Public::Lists#new</h1>
<p>Find me in app/views/public/lists/new.html.erb</p>

<h1>新規投稿</h1>
<%= form_with model: @list, url: '/lists', method: :post, local: true do |f| %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>

	<h4>材料</h4>
  <%= f.text_field :material %>

  <h4>本文</h4>
  <%= f.text_area :body %>

  <%= f.submit '投稿' %>
<% end %>